    "cross-spawn": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
