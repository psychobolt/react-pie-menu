{
  "name": "@psychobolt/react-rollup-boilerplate",
  "version": "0.0.2",
  "description": "A boilerplate for building React libraries",
  "type": "module",
  "module": "./dist/index.prod.js",
  "main": "./dist/index.prod.cjs",
  "exports": {
    "development": {
      "module": "./dist/index.dev.js",
      "import": "./dist/index.dev.cjs.mjs",
      "require": "./dist/index.dev.cjs"
    },
    "import": "./dist/index.prod.cjs.mjs",
    "require": "./dist/index.prod.cjs",
    "default": "./dist/index.prod.js"
  },
  "repository": "https://github.com/psychobolt/react-rollup-boilerplate",
  "author": "psychobolt",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "flow-typed-update": "yarn node flow-typed-update.js",
    "flow-typed-install": "rimraf flow-typed/npm && yarn run flow-typed-update",
    "build:dev": "cross-env BABEL_ENV=development rollup -c rollup.config.dev.mjs",
    "build:prod": "cross-env rollup -c rollup.config.prod.mjs",
    "clean": "concurrently \"rimraf dist/index.*.?(c)js\" \"rimraf dist/*-*.*.?(c)js\" \"rimraf packages/**/dist/index.*.?(c)js\" \"rimraf packages/**/dist/*-*.*.?(c)js\"",
    "build": "yarn run clean && yarn run build:dev && yarn run build:prod",
    "watch": "yarn run build:dev --w",
    "watch:prod": "yarn run build:prod --w",
    "dev": "cross-env BABEL_ENV=development concurrently \"npm:watch\" \"npm:storybook\"",
    "start": "concurrently \"npm:watch:prod\" \"npm:storybook\"",
    "test": "cross-env BABEL_ENV=test yarn node --experimental-vm-modules $(yarn bin jest) --passWithNoTests",
    "lint": "yarn run lint:css && yarn run lint:js",
    "lint:css": "stylelint ./(packages|stories|src)/**/*.js",
    "lint:js": "eslint \"**/*.js\"",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook",
    "bootstrap": "yarn node bootstrap.js",
    "prepack": "yarn run build",
    "dist": "yarn workspaces foreach --no-private npm publish --tolerate-republish"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/helper-compilation-targets": "^7.16.7",
    "@babel/node": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/plugin-proposal-do-expressions": "^7.14.0",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-proposal-json-strings": "^7.13.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-numeric-separator": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.13.12",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/plugin-proposal-private-property-in-object": "^7.14.0",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/plugin-transform-modules-commonjs": "^7.14.0",
    "@babel/plugin-transform-react-jsx": "^7.16.7",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-flow": "^7.13.13",
    "@babel/preset-react": "^7.13.13",
    "@babel/template": "^7.16.7",
    "@mdx-js/react": "1.6.22",
    "@psychobolt/default-export": "workspace:^",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^4.0.0",
    "@storybook/addon-actions": "^6.5.0-alpha.46",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-docs": "^6.5.0-alpha.46",
    "@storybook/addon-essentials": "^6.5.0-alpha.46",
    "@storybook/addon-links": "^6.5.0-alpha.46",
    "@storybook/builder-webpack5": "^6.5.0-alpha.46",
    "@storybook/cli": "^6.5.0-alpha.46",
    "@storybook/manager-webpack5": "^6.5.0-alpha.46",
    "@storybook/react": "^6.5.0-alpha.46",
    "@storybook/theming": "^6.5.0-alpha.46",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "@yarnpkg/core": "^3.2.0-rc.15",
    "app-root-path": "^3.0.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "babel-plugin-styled-components": "^2.0.2",
    "cash-true": "^0.0.2",
    "codecov": "^3.8.1",
    "concurrently": "^7.0.0",
    "cosmiconfig": "^7.0.1",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "flow-bin": "^0.171.0",
    "flow-typed": "^3.3.1",
    "glob": "^7.1.6",
    "is-class": "^0.0.9",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "jest-styled-components": "^7.0.8",
    "lodash": "^4.17.21",
    "micromatch": "^4.0.4",
    "postcss": "^8.4.6",
    "postcss-scss": "^4.0.3",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^17.0.2",
    "react-cache": "workspace:^",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.2",
    "rollup-plugin-mjs-entry": "^0.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "semver": "^7.3.5",
    "slash": "3.0.0",
    "source-map-loader": "^3.0.1",
    "styled-components": "^5.2.3",
    "stylelint": "^14.5.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "https://github.com/psychobolt/stylelint-processor-styled-components",
    "yargs": "^17.3.1"
  },
  "dependencies": {
    "react-is": "^17.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.2.3"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "packageManager": "yarn@3.2.0-rc.16"
}
